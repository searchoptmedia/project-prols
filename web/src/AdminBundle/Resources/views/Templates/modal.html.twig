<!-----------------------SESSION EXPIRE CONTAINER------------------------->
<div class="modal-container modal-logout-notify-container" style="display:none;">
    <div class="time-in center-align">
        <div class="valign-wrapper">

            <div class="prompt" style="display:block; width:100%; float:none; clear:both;">
                <p class="confirm-timein"><b>Oops!</b></p>
                <p class="confirm-timein">Something went wrong.</p>
                <p class="confirm-timein error-message" style="color: #fd7a7a;"></p>
                <div class="btn-block">
                    <a class="waves-effect waves-light red btn-flat btn-large btn-timein" href="/">RELOAD PAGE</a>
                </div>
            </div>
        </div>
    </div>
</div>

<!-----------------------FAIL AJAX CONTAINER------------------------->
<div class="modal-container modal-fail-ajax-container" style="display:none;">
    <div class="time-in center-align">
        <div class="valign-wrapper">

            <div class="prompt" style="display:block; width:100%; float:none; clear:both;">
                <p class="confirm-timein"><b>Oops!</b></p>
                <p class="confirm-timein">Something went wrong.</p>
                <br>
                <div class="btn-block">
                    <a class="waves-effect waves-light red btn-flat btn-large btn-timein" href="/">RELOAD PAGE</a>
                </div>
            </div>
        </div>
    </div>
</div>

<!-----------------------BIRTHDAY MODAL CONTAINER------------------------->
<div class="plr-modal custom-modal-1 modal-container modal-birthday-notify-container display-none" style="background: rgba(0, 0, 0, 0.50);" >
    <div class="center-align" style="position: relative; top: 0px;">
        <div class="valign-wrapper">

            <div class="prompt" style="display:block; width:100%; float:none; clear:both;">
                <p class="confirm-timein"><b>Happy Birthday!!!</b><br>
                    <span class="-txt-bday-names"></span>
                </p>
                <p class="confirm-timein" style="font-size: 30px; position:relative; top: 20px;">Greetings from your <b>Propelrr</b> Family!<br>Burger! Burger!</p>
                <div class="btn-block" style="position:relative;top: 20px;">
                    <a class="waves-effect waves-light green btn-flat btn-large button-action -btn-close-birthday" href="#">CLOSE</a>
                </div>
            </div>
        </div>
    </div>
</div>

<!-----------------------TIME IN MODAL CONTAINER------------------------->
{#<div class="plr-modal custom-modal-1 modal-time-in-container allowed {% if timeinStatus=='not-timein-allow-ip' and autoTimeout==0 %}{% else %}display-none{% endif %}">#}
<div class="plr-modal custom-modal-1 modal-time-in-container allowed display-none">
    <div class="time-in center-align">
        <div class="valign-wrapper">

            <div class="prompt" style="display:block; width:100%; float:none; clear:both;">
                <div class="clock2">
                    <div class="widget-big-int plugin-clock">00:00</div>
                    <div class="widget-subtitle plugin-date">Loading...</div>
                </div>
                <h3 class="-modal-title -modal-greetings-text">{{ timeData.greetings }}</h3>
                <img class="-modal-loader-img" src="{{ asset('prols2/img/ajax-loader.gif') }}" style="width: 20px; height: 20px;">
                <div class="btn-block">
                    <a class="waves-effect waves-light green btn-flat btn-large button-action -btn-timein-submit display-none"
                       id="time-in-button" data-action="timein" data-id="{{ app.user.id }}">
                        <i class="material-icons left icon-timein">query_builder</i>Time in
                    </a>
                </div>
            </div>
        </div>
    </div>
</div>

<!-- if not same ip -->
{#<div class="plr-modal custom-modal-1 modal-time-in-container deny {% if timeinStatus=='not-timein-deny-ip' and autoTimeout==0 %}{% else %}display-none{% endif %}">#}
<div class="plr-modal custom-modal-1 modal-time-in-container deny display-none">
    <div class="diff-ip center-align">
        <div class="valign-wrapper">

            <div class="prompt" style="display:block; width:100%; float:none; clear:both;">
                <div class="clock2"><div class="widget-big-int plugin-clock">00:00</div>
                    <div class="widget-subtitle plugin-date">Loading...</div></div>
                <p class="diff-ip-notice" style="font-weight:700">You logged in with a different IP Address!</p>
                <textarea id="diffip-input-reason" class="materialize-textarea -txt-timein-reason" style="width: 500px; color:white; text-align:center; font-weight:400" placeholder="Enter Message..."></textarea>
                <br>
                <img class="-modal-loader-img" src="{{ asset('prols2/img/ajax-loader.gif') }}" style="width: 20px; height: 20px;">
                <div class="btn-block">
                    <a class="waves-effect waves-light green btn-flat btn-large button-action -btn-timein-submit display-none"
                       id="time-in-button" data-action = "timein" data-id="{{ app.user.id }}">
                        <i class="material-icons left icon-timein">query_builder</i>Time in</a>
                </div>
            </div>
        </div>
    </div>
</div>


<!-----------------------TIME OUT MODAL CONTAINER------------------------->
<div class="plr-modal custom-modal-1 modal-timeout-container display-none">
    <div class="center-align">
        <div class="valign-wrapper">
            <div class="prompt" style="display:block; width:100%; float:none; clear:both;">
                <div class="clock2"><div class="widget-big-int plugin-clock">00:00</div>
                    <div class="widget-subtitle plugin-date">Loading...</div>
                </div>
                <h3 class="-modal-title">Are you sure you want to time-out?</h3><br>
                <div class="-timeout-error-message display-none" style="color: #f44336;"><strong>Wrong Password</strong></div>
                <input class="text-center font20 -txt-timeout-password" type="password" placeholder="Enter password...">
                <img class="-modal-loader-img" src="{{ asset('prols2/img/ajax-loader.gif') }}" style="width: 20px; height: 20px;">
                <div class="btn-block">
                    <a class="waves-effect waves-light green btn-flat btn-large button-action -btn-timeout-confirm display-none">Yes</a> &nbsp;
                    <a class="waves-effect waves-light red btn-flat btn-large button-action -btn-timeout-cancel ">Cancel</a>
                </div>
            </div>
        </div>
    </div>
</div>

<!-----------------------LOG OUT MODAL CONTAINER------------------------->
<div class="logout-container">
    <div class="logout center-align">
        <div class="valign-wrapper">
            <div class="prompt" style="display:block; width:100%; float:none; clear:both;">
                <p class="confirm-logout">Are you sure you want to log-out?</p>
                <div class="btn-block">
                    <a class="waves-effect waves-light green btn-flat btn-large btn-yes-logout" href="{{ path('logout') }}">Yes</a>&nbsp;&nbsp;
                    <a class="waves-effect waves-light red btn-flat btn-large btn-cancel">Cancel</a>
                </div>
            </div>
        </div>
    </div>
</div>

<!-----------------------FORGOT TO TIMEOUT MODAL CONTAINER------------------------->
<div class="modal-container forgot-timeout-modal display-none">
    <div class="center-align" style="position: relative; top: 350px;">
        <div class="valign-wrapper">

            <div class="prompt auto-timeout-prompt" style="display:block; width:100%; float:none; clear:both;">
                <p class="confirm-timein"><strong>It's another day!</strong></p>
                <br>
                <br>
                <div class="btn-block">
                    <a class="waves-effect waves-light red btn-flat btn-large btn-timeout btn-timeout-newday" style="height: 54px; line-height: 55px; top: 0px;">
                        <i class="material-icons left">query_builder</i>Time Out</a>
                    &nbsp;
                    <a class="waves-effect waves-light green btn-flat btn-large forgot-timeout-btn btn-keep-working" style="color: white; ">
                        <i class="material-icons left">work</i>I'M STILL WORKING<span id="time">(30)</span></a>
                </div>
            </div>

            <div class="prompt time-in-prompt" style="display:none; width:100%; float:none; clear:both;">

                <div class="btn-block">

                </div>
            </div>
        </div>
    </div>
</div>

<!-----------------------AUTO TIME OUT MODAL CONTAINER------------------------->
<div class="plr-modal modal-container modal-auto-timeout-notice {% if autoTimeout==0 %}display-none{% endif %}">
    <div class="center-align" style="position: relative; top: 0%;">
        <div class="valign-wrapper">
            <div class="prompt auto-timeout-prompt" style="display:block; width:100%; float:none; clear:both;">
                <p class="confirm-timein">You have been timed out automatically!</p>
                <p class="confirm-timein" style="font-size: 24px">Timed out at <span class="-txt-autotimeout-message-out">{{ app.session.get('autoTimeOutDate')|date('M d, Y') }} 12:00 AM</span></p>
                <p class="confirm-timein" style="font-size: 24px">Last Timed in at <span class="-txt-autotimeout-message-in"></span></p>
                <br><br>
                <img class="-modal-loader-img" src="{{ asset('prols2/img/ajax-loader.gif') }}" style="width: 20px; height: 20px;">
                <div class="btn-block">
                    <a class="waves-effect waves-light red btn-flat btn-large button-action -btn-auto-timeout-timein display-none">
                        <i class="material-icons left">query_builder</i>Time In</a>
                </div>
            </div>
        </div>
    </div>
</div>
